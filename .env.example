# AWS Configuration
# Get these from AWS Console or AWS Academy
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here  # Optional, for temporary credentials
AWS_REGION=us-east-1

# AWS Bedrock Model Configuration
# Using Claude 3.5 Haiku with cross-region inference profile
BEDROCK_MODEL_ID=us.anthropic.claude-3-5-haiku-20241022-v1:0

# DynamoDB Table Names
DYNAMODB_TABLE_PLANS=PatchPlans-dev
DYNAMODB_TABLE_PATCH_RUNS=PatchRuns-dev
DYNAMODB_TABLE=PatchPilotExecutions-dev

# SuperOps Integration (Optional - currently using mock data)
SUPEROPS_API_URL=https://api.superops.ai
SUPEROPS_API_KEY=your_superops_api_key_here

